          {% image "img/Section7Image.png", pageData.section[7].img_alt, "400, 800, 1200" %}
        </div>
      </div>

      <p class="section-para-text text-left">
        {{ pageData.section[7].paragraph[0] }}
      </p>
      <p class="section-para-text text-left">
        {{ pageData.section[7].paragraph[1] }}
      </p>
    </div>
  </div>
</div>

<!-- Sektion 9 p/img -->
<div class="container my-5 mt-lg-n4">
  <div class="d-flex flex-row align-items-center justify-content-between">
    <div class="px-1">
      <h2 class="section-header-text">
        {{ pageData.section[8].heading }}
      </h2>
      <div class="d-lg-none col-8 col-md-6 mx-auto pb-3">
        <div class="rounded overflow-hidden">
          {% image "img/people-group-drawing.jpg", pageData.section[8].img_alt, "400, 800, 1200" %}
        </div>
      </div>
      <p class="section-para-text text-left">
        {{ pageData.section[8].paragraph }}
      </p>
    </div>
    <div class="d-none d-lg-block col-lg-4">
      <div class="rounded overflow-hidden">
        {% image "img/people-group-drawing.jpg", pageData.section[8].img_alt, "400, 800, 1200" %}
      </div>
    </div>
  </div>
</div>

<!-- Sektion 10 Det med småt p/img -->
