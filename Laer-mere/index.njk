</div>

<!-- Sektion 7 Tilgængelighed giver et hav af fordele -->
<div class="container my-5">
  <h2 class="fullwidth-sm section-header-text mb-0">
    {{ pageData.section[6].heading }}
  </h2>
  <div class="row d-flex flex-row flex-wrap justify-content-between">
    {{
      iconcard({
        card_class:
          "card col-sm-12 col-md-4 col-xl-3 border-0 my-2 pr-md-4 pr-xl-0",
        icon: "bi bi-people",
        heading: pageData.section[6].card[0].heading,
        body: pageData.section[6].card[0].paragraph
      })
    }}

    {{
      iconcard({
        card_class:
          "card col-sm-12 col-md-4 col-xl-3 border-0 my-2 pr-md-4 pr-xl-0",
        icon: "bi bi-person-check",
        heading: pageData.section[6].card[1].heading,
        body: pageData.section[6].card[1].paragraph
      })
    }}

    {{
      iconcard({
        card_class:
          "card col-sm-12 col-md-4 col-xl-3 border-0 my-2 pr-md-4 pr-xl-0",
        icon: "bi bi-book",
        heading: pageData.section[6].card[2].heading,
        body: pageData.section[6].card[2].paragraph
      })
    }}
  </div>

  <div class="row d-flex flex-row flex-wrap justify-content-between">
    {{
      iconcard({
        card_class:
          "card col-sm-12 col-md-4 col-xl-3 border-0 my-2 pr-md-4 pr-xl-0",
        icon: "bi bi-google",
        heading: pageData.section[6].card[3].heading,
        body: pageData.section[6].card[3].paragraph
      })
    }}

    {{
      iconcard({
        card_class:
          "card col-sm-12 col-md-4 col-xl-3 border-0 my-2 pr-md-4 pr-xl-0",
        icon: "bi bi-megaphone",
        heading: pageData.section[6].card[4].heading,
        body: pageData.section[6].card[4].paragraph
      })
    }}

    {{
      iconcard({
        card_class:
          "card col-sm-12 col-md-4 col-xl-3 border-0 my-2 pr-md-4 pr-xl-4",
        icon: "bi bi-arrow-up-circle",
        heading: pageData.section[6].card[5].heading,
        body: pageData.section[6].card[5].paragraph
      })
    }}
  </div>
</div>

<!-- Sektion 8 Hvad siger loven p/img -->
<div class="container">
  <div class="row d-flex flex-row align-items-center justify-content-between my-5">
    <div class="d-none d-lg-flex col-12 col-lg-5">
      <div class="rounded overflow-hidden">
